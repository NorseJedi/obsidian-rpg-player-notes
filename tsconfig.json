{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ES2024",
		"allowJs": true,
		"strict": true,
		"noImplicitAny": true,
		"moduleResolution": "bundler",
		"importHelpers": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"paths": {
			"@/*": ["./src/*"],
			"obsidian-typings/implementations": [
				"./node_modules/obsidian-typings/dist/implementations.d.ts",
				"./node_modules/obsidian-typings/dist/implementations.cjs"
			]
		},
		"lib": [
			"DOM",
			"ES5",
			"ES6",
			"ES7",
			"ScriptHost",
			"ES2024"
		],
		"types": [
			"node",
			"obsidian-typings",
			"obsidian"
		]
	},
	"include": [
		"**/*.ts",
		"**/*.d.ts"
	]
}
